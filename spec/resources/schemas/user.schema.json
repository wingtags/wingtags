{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "CreatedDate": {
      "type": "integer",
      "minimum": 1000000000000,
      "maximum": 9999999999999
    },
    "DeviceUID": {
      "type": "string"
    },
    "Email": {
      "type": "string",
      "format": "email"
    },
    "FirstName": {
      "type": "string"
    },
    "LastName": {
      "type": "string"
    },
    "SpotterID": {
      "type": "string"
    }
  },
  "required": [
    "CreatedDate",
    "DeviceUID",
    "Email",
    "FirstName",
    "LastName",
    "SpotterID"
  ]
}